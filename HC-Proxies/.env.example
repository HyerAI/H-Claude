# ============================================
# H-Claude Proxies - Shared Configuration
# ============================================
# Copy this to .env and fill in your API keys
# Restart proxies: .claude/scripts/stop-proxies.sh && .claude/scripts/start-proxies.sh

# --------------------------------------------
# Google AI API Keys
# --------------------------------------------
# Get from: https://aistudio.google.com/apikey

# Primary key (Flash workers: HC-Work, HC-Work-R, HC-Orca)
GOOGLE_AI_API_KEY_PRIMARY=your-primary-key-here

# Secondary key (Pro reasoning: HC-Reas-B, HC-Orca-R)
# Optional - uses PRIMARY if not set
GOOGLE_AI_API_KEY_SECONDARY=your-secondary-key-here

# Image generation key (CG-Image)
GOOGLE_API_KEY_IMAGE=your-image-key-here

# --------------------------------------------
# Model Configuration
# --------------------------------------------
# Flash models (fast execution)
MODEL_FLASH=gemini-2.5-flash-preview-05-20

# Pro models (reasoning)
MODEL_PRO=gemini-2.5-pro-preview-05-06

# --------------------------------------------
# Port Configuration
# --------------------------------------------
HC_REAS_A_PORT=2410
HC_REAS_B_PORT=2411
HC_WORK_PORT=2412
HC_WORK_R_PORT=2413
HC_ORCA_PORT=2414
HC_ORCA_R_PORT=2415
CG_IMAGE_PORT=2407

# --------------------------------------------
# Timeouts (ms)
# --------------------------------------------
HC_REAS_A_TIMEOUT=300000
DEFAULT_TIMEOUT=120000
